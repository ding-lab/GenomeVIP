
# Core and UI dependency: http://en.wikipedia.org/wiki/JQuery_UI

wget http://code.jquery.com/jquery-1.8.3.min.js
mkdir -p ui/1.10.4
cd ui/1.10.4
wget -O ui/1.10.4/jquery-ui.js     http://code.jquery.com/ui/1.10.4/jquery-ui.js
wget -O ui/1.10.4/jquery-ui.min.js http://code.jquery.com/ui/1.10.4/jquery-ui.min.js
mkdir -p  ui/1.10.4/themes/smoothness
wget -O ui/1.10.4/themes/smoothness/jquery-ui.css    http://code.jquery.com/ui/1.10.4/themes/smoothness/jquery-ui.css 
